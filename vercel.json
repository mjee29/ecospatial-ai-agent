{
  "rewrites": [
    {
      "source": "/wms/:path*",
      "destination": "https://climate.gg.go.kr/ols/api/geoserver/wms/:path*"
    },
    {
      "source": "/wfs/:path*",
      "destination": "https://climate.gg.go.kr/ols/api/geoserver/wfs/:path*"
    },
    {
      "source": "/airkorea/:path*",
      "destination": "https://apis.data.go.kr/:path*"
    },
    {
      "source": "/sgis-data/:path*",
      "destination": "https://sgisapi.mods.go.kr/:path*"
    },
    {
      "source": "/gg-openapi/:path*",
      "destination": "https://openapi.gg.go.kr/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
